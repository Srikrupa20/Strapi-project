{
  "kind": "collectionType",
  "collectionName": "ps",
  "info": {
    "singularName": "p",
    "pluralName": "ps",
    "displayName": "Holiday trip",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text",
      "required": false
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "destination": {
      "type": "enumeration",
      "enum": [
        "Karnataka",
        "Kashmir",
        "Kerala"
      ],
      "default": "Karnataka",
      "required": true
    },
    "places": {
      "type": "enumeration",
      "enum": [
        "Hills",
        "River",
        "Falls"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    }
  }
}
